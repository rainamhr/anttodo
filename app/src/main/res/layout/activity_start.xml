<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/start_activity"
    android:layout_marginLeft="20dp"
    android:layout_marginRight="20dp"
    android:layout_marginTop="25dp"
    android:layout_marginBottom="50dp"
    android:layout_height="match_parent"
    tools:context="com.example.queen.saediproject.startActivity">

    <TextView
        android:id="@+id/percent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="25%"
        android:textColor="#fff"
        android:textSize="110dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/find"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/percent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="128dp"
        android:text="@string/and"
        android:textColor="#fff"
        android:textSize="23dp" />

    <ImageView
        android:id="@+id/imagegorstart"
        android:layout_below="@+id/find"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:src="@drawable/object"/>

</RelativeLayout>
